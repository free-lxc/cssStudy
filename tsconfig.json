{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "strict": true,
    "noUnusedLocals": false, //不检查未使用的变量
    "noUnusedParameters": false, //不检查未使用的参数
    "noImplicitAny": false, // 允许any类型
    "noUnusedVars": false,
    //配置 @
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
